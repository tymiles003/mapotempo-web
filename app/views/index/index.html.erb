<% if user_signed_in? %>
  <% if @customer || current_user.admin? %>
    <% if current_user.admin? || (!@customer.end_subscription || @customer.end_subscription > Time.now) %>
      <%= render 'subscription_on' %>
    <% else %>
      <%= render 'subscription_end' %>
    <% end %>
  <% end %>
<% else %>
  <%= render 'default_home_page' %>
<% end %>
